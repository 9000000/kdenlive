<!DOCTYPE kpartgui>
<effect tag="oldfilm" id="grain" type="video" unique="1">
	<name>Grain</name>
	<description>Grain over the image</description>
	<author>Marco Gittler</author>
	<parameter type="integer" name="noise" max="200" min="0" default="40">
		<name>Noise</name>
	</parameter>
	<parameter type="integer" name="contrast" max="400" min="0" default="160">
		<name>Contrast</name>
	</parameter>
	<parameter type="integer" name="brightness" max="400" min="0" default="70">
		<name>Brightness</name>
	</parameter>
</effect>
